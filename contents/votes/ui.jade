h5 Search
.row
    .col-md-8
        .form-group.form-group-default
            label Year            
            input#year.form-control(type='text', required='')                
    .col-md-4                         
        button.btn.btn-primary(onClick='votes.searchByYear($("#year").val())') Search

.row
    .col-md-8
        .form-group.form-group-default
            label Number            
            input#number.form-control(type='text', required='')                
    .col-md-4                         
        button.btn.btn-primary(onClick='votes.searchByNumber($("#number").val())') Search


